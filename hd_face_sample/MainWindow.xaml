<Window x:Class="hd_face_marker_tracking.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hd_face_marker_tracking"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid Background="Black">
        <Image x:Name="ColorImage" HorizontalAlignment="Left" Height="424" Margin="10,10,-108,0"
               VerticalAlignment="Top" Width="512"/>
        <!-- <Viewbox Grid.Row="1"> -->
        <Canvas Name="canvas" HorizontalAlignment="Left" Height="424" Margin="10,10,-108,0"
            VerticalAlignment="Top" Width="512"/>
        <Canvas Name="canvas_blob" HorizontalAlignment="Left" Height="424" Margin="10,10,-108,0"
            VerticalAlignment="Top" Width="512"/>
        <Label x:Name="label_sample" Content="Label" HorizontalAlignment="Left" Margin="25,524,0,0" 
               VerticalAlignment="Top" Width="757" Height="35" Foreground="White"/>
        <Label x:Name="label_coords" Content="Label_coords" HorizontalAlignment="Left" Margin="565,10,0,0" 
               VerticalAlignment="Top" Height="550" Width="215" Foreground="White"/>

        <!--</Viewbox> -->
    </Grid>

</Window>
